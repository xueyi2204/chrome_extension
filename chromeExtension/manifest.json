{
  "manifest_version": 2,
  "name": "My Extension",
  "version": "1.0",
  "description": "Traverse div elements with class 'filter-layout-content'",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "popup.png"
  }
}
